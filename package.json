{
  "name": "angular-graphql-message-bus",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "graphql": "npm run graphql:schema && npm run graphql:introspect",
    "graphql:schema": "npx get-graphql-schema https://graphql-message-bus.now.sh/ > src/app/generated/schema.graphql",
    "graphql:introspect": "npx gql-gen --schema https://graphql-message-bus.now.sh/ --template graphql-codegen-apollo-angular-template --out src/app/generated/graphql.ts src/app/graphql/*.graphql"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "apollo-angular": "^1.3.0",
    "apollo-angular-link-http": "^1.2.0",
    "apollo-cache-inmemory": "^1.2.0",
    "apollo-client": "^2.4.0",
    "apollo-link": "^1.2.0",
    "apollo-link-ws": "^1.0.9",
    "apollo-utilities": "^1.0.21",
    "bootswatch": "^4.1.3",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "rxjs": "^6.0.0",
    "subscriptions-transport-ws": "^0.9.14",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.2",
    "@angular/cli": "~6.2.2",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.10.1",
    "codelyzer": "~4.4.4",
    "get-graphql-schema": "^2.1.1",
    "graphql-code-generator": "^0.12.2",
    "graphql-codegen-apollo-angular-template": "^0.12.2",
    "graphql-codegen-introspection-template": "^0.12.2",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.1",
    "tslint": "~5.11.0",
    "typescript": "2.9.2"
  }
}
